   "source": [
    "# Write an SQL query to show the top n (say 10) records of a table."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e33c5cdb",
   "metadata": {},
   "outputs": [],
   "source": [
