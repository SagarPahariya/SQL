   "source": [
    "# Write an SQL query to determine the nth (say n=5) highest salary from a table."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "11c7417b",
   "metadata": {},
   "outputs": [],
   "source": [
    "select * from table_name order by salary desc limit 5;"
   ]
  }
